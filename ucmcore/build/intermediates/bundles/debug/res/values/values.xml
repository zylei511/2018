    <style name="AlertActivity_AlertStyle" parent="android:Theme.Dialog">
        <!--<item name="android:windowAnimationStyle">@style/AnimAlert</item>-->
        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 半透明 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 背景透明 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 模糊 -->
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="CRMAppTheme" parent="android:Theme.Light">
        <!-- Customize your theme here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:buttonBarButtonStyle">?android:attr/borderlessButtonStyle</item>
        <!--<item name="colorPrimaryDark">@color/colorPrimaryDark</item>-->
        <!--<item name="colorAccent">@color/colorAccent</item>-->
    </style>
    <style name="create_work_anim" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/crm_sdk_pop_enter_anim</item>
        <item name="android:windowExitAnimation">@anim/crm_sdk_pop_exit_anim</item>
    </style>
    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowIsFloating">true</item>
        <!-- 设置未浮动窗口 -->
        <item name="android:windowFrame">@null</item>
        <!-- 设置无边框 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 设置无标题 -->
        <item name="android:windowBackground">@color/crm_sdk_sc_transparent_background</item>
        <!-- 设置完全透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 设置屏幕变暗 -->
    </style>
    <style name="popUpWindowAnim">
        <item name="android:windowEnterAnimation">@anim/crm_sdk_slide_in_bottom</item>
        <item name="android:windowExitAnimation">@anim/crm_sdk_slide_out_bottom</item>
    </style>
</resources>